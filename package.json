    "husky": "^5.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.8",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^10.5.4",
    "sonarqube-scanner": "^2.8.0",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
