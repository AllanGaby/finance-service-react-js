  "scripts": {
    "build": "webpack --config webpack.development.js ",
    "dev:server": "webpack serve --config webpack.development.js --open",
    "sonar:scanner": "npm run test:ci && npx sonar-scanner -Dsonar.host.url=$SONAR_HOST -Dsonar.projectKey=REACT-TS-SKELETON",
